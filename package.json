{
  "name": "b-pl-tree",
  "version": "1.0.1",
  "description": "Pretty simple b+ tree implementation on typescript",
  "main": "dist/index.js",
  "typings": "types/index.d.ts",
  "types": "types/index.d.ts",
  "scripts": {
    "prepublish": "tsc --build tsconfig.json",
    "clean": "git clean -dfqX -- ./node_modules **/{dist,node_modules}/",
    "compile": "tsc --build tsconfig.json",
    "compile:clean": "tsc --build tsconfig.json --clean",
    "watch": "tsc --build tsconfig.json --watch",
    "watch-c": "node tools/watch.js",
    "watch-b": "ts-node-script tools/watch-benchmark.ts",
    "build": "node tools/build.js",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vedmalex/b-plus-tree.git"
  },
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "axios": "^1.2.2",
    "benchmark": "^2.1.4",
    "bintrees": "^1.0.2",
    "chokidar": "^3.5.3",
    "commander": "^9.4.1",
    "esbuild": "^0.16.14",
    "esbuild-plugin-ifdef": "^1.0.1",
    "eslint": "^8.31.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-watch": "^8.0.0",
    "estrella": "^1.4.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "sorted-btree": "^1.8.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {},
