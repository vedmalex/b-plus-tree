{
  "name": "b-pl-tree",
  "version": "1.0.0-aplfa.0",
  "description": "Pretty simple b+ tree implementation on typescript",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "types": "dost/index.d.ts",
  "scripts": {
    "prepublish": "tsc --build tsconfig.json",
    "clean": "git clean -dfqX -- ./node_modules **/{dist,node_modules}/",
    "compile": "tsc --build tsconfig.json",
    "compile:clean": "tsc --build tsconfig.json --clean",
    "watch": "tsc --build tsconfig.json --watch",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vedmalex/b-plus-tree.git"
  },
  "devDependencies": {
    "@types/node": "^14.14.20",
    "benchmark": "^2.1.4",
    "bintrees": "^1.0.2",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "mongo-objectid": "^1.2.1"
  },
  "author": "Alex Vedmedenko ",
  "license": "MIT"
}