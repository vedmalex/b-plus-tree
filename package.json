{
  "name": "b-pl-tree",
  "version": "1.0.0-rc.3",
  "description": "Pretty simple b+ tree implementation on typescript",
  "main": "dist/index.js",
  "typings": "types/index.d.ts",
  "types": "types/index.d.ts",
  "scripts": {
    "prepublish": "tsc --build tsconfig.json",
    "clean": "git clean -dfqX -- ./node_modules **/{dist,node_modules}/",
    "compile": "tsc --build tsconfig.json",
    "compile:clean": "tsc --build tsconfig.json --clean",
    "watch": "tsc --build tsconfig.json --watch",
    "watch-c": "node tools/watch.js",
    "watch-b": "ts-node-script tools/watch-benchmark.ts",
    "build": "node tools/build.js",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vedmalex/b-plus-tree.git"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "axios": "^0.21.1",
    "benchmark": "^2.1.4",
    "bintrees": "^1.0.2",
    "chokidar": "^3.5.1",
    "commander": "^7.0.0",
    "esbuild": "^0.8.36",
    "esbuild-plugin-ifdef": "^1.0.1",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint-watch": "^7.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "sorted-btree": "^1.4.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {},
